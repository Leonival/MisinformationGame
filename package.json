  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider craco start",
    "emulators": "npx firebase emulators:start --import=\"test/firebase-dev-environment\"",
    "docs": "cd docs && bundle-2.7 exec jekyll serve --baseurl=\"\" -o --watch",
    "build": "NODE_OPTIONS=--openssl-legacy-provider craco build",
    "test": "NODE_OPTIONS=--openssl-legacy-provider craco test",

    "ci-build": "NODE_OPTIONS=--openssl-legacy-provider CI=false INLINE_RUNTIME_CHUNK=false craco build",

    "win-start": "craco start",
